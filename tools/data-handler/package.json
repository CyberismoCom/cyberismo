    "pino": "^9.9.5",
    "simple-git": "^3.28.0",
    "string-pixel-width": "^1.11.0",
    "tslib": "^2.6.2",
    "vega": "^6.1.2",
    "vega-lite": "^6.3.1",
    "write-json-file": "^7.0.0"
